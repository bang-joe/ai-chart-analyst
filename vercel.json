{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/manifest.json", "dest": "/public/manifest.json" },
    { "src": "/service-worker.js", "dest": "/public/service-worker.js" },
    { "src": "/offline.html", "dest": "/public/offline.html" },
    { "src": "/favicon-16x16.png", "dest": "/public/favicon-16x16.png" },
    { "src": "/favicon-32x32.png", "dest": "/public/favicon-32x32.png" },
    { "src": "/apple-touch-icon.png", "dest": "/public/apple-touch-icon.png" },
    { "src": "/icon-192.png", "dest": "/public/icon-192.png" },
    { "src": "/icon-512.png", "dest": "/public/icon-512.png" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
